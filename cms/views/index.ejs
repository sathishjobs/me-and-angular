                                <input type="tel" ng-model="live.phone" class="singleLineForm" id="phone" required>
                            </div>
                            <div class="col-xs-12 col-sm-6 formPair">
                                <label for="age">Age<span class="redAsterisk">*</span></label>
                                <input type="number" ng-model="live.age" class="singleLineForm" id="age" required>
                            </div>
                        </div>
                        <div class="form-group" style="position:relative">
                            <div class="col-xs-12 col-sm-6 formPair">
                                <label>Cover Letter</label>
                                <textarea ng-model="live.coverLetter" class="myTextArea" rows="10"></textarea>
                            </div>
                            <!-- <div class="col-xs-12 col-sm-6 formPair" style="position:static">
