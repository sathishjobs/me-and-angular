            angular.extend(toastrConfig, {
                autoDismiss: false,
                containerId: 'toast-container',
                maxOpened: 0,
                newestOnTop: true,
                positionClass: 'toast-bottom-right',
                preventDuplicates: false,
                preventOpenDuplicates: false,
                target: 'body'
            });
        })
        .controller('mainController', ['$scope', '$http', 'toastr', function($scope, $http, toastr) {
            $scope.liveSend = function() {
                console.log('live there::', $scope.live);
                $http.post('/liveForm', $scope.live).then(function(res) {
                    $scope.live = {};
                    console.log('liveForm response::::', res);
                    toastr.success(res.data);
                }, function(err) {
                    toastr.error(err.data);
                    console.log('Error', err);
                });
            };
            $scope.talkSend = function() {
                console.log('talk:::', $scope.talk);
                $http.post('/talkForm', $scope.talk).then(function(res) {
                    toastr.success(res.data);
                    $scope.talk = {};
                    console.log('talkForm:::', res);
                }, function(err) {
                    toastr.error(err.data);
                    console.log('Error', err);
                });
            };
        }]);
